{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "esModuleInterop": true,
        "jsx": "react",
        "lib": [
            "dom",
            "es7"
        ],
        "module": "es6",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "target": "es6",
        "types": [
            "@material-ui/core"
        ]
    },
    "exclude": [
        "./node_modules",
        "dist"
    ]
}
